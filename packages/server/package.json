{
  "name": "browser-rpc",
  "version": "0.0.8",
  "description": "Local RPC proxy that routes Ethereum transactions through your browser wallet",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gskril/browser-rpc.git",
    "directory": "packages/server"
  },
  "main": "dist/index.js",
  "bin": {
    "browser-rpc": "dist/index.js"
  },
  "files": [
    "dist",
    "web-dist",
    "README.md"
  ],
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target node",
    "start": "bun run src/index.ts",
    "start:bundled": "bun run dist/index.js",
    "sync-web": "rm -rf web-dist && cp -r ../web/dist ./web-dist"
  },
  "dependencies": {
    "@hono/node-server": "1.19.8",
    "hono": "4.11.3",
    "commander": "14.0.2"
  },
  "devDependencies": {
    "@types/node": "22.19.5",
    "typescript": "5.9.3"
  }
}
